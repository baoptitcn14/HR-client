                "code": "exp_description",
                "inputType": "editor",
                "fieldIndex": 4,
                "_inputConfigOpenJson": {
                    "showToolbar": true,
                    "showAction": true,
                    "actionConfig": {
                        "showMoveUp": true,
                        "showMoveDown": true,
                        "showDelete": false,
